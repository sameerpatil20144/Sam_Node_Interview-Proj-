      // /header_top
      .header-middle
        // header-middle
        .container
          .row
            .col-sm-4
              .logo.pull-left
                a(href='index.html')
                  img(src='/public/images/home/logo.png', alt='')
              .btn-group.pull-right
                .btn-group
                  button.btn.btn-default.dropdown-toggle.usa(type='button', data-toggle='dropdown')
                    | USA
                    span.caret
                  ul.dropdown-menu
                    li
                      a(href='#') Canada
                    li
                      a(href='#') UK
                .btn-group
                  button.btn.btn-default.dropdown-toggle.usa(type='button', data-toggle='dropdown')
                    | DOLLAR
                    span.caret
                  ul.dropdown-menu
                    li
                      a(href='#') Canadian Dollar
                    li
                      a(href='#') Pound
            .col-sm-8
              .shop-menu.pull-right
                ul.nav.navbar-nav
                  li
