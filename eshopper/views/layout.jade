                h2 Brands
                .brands-name
                  ul.nav.nav-pills.nav-stacked
                    li(ng-repeat = "ans in records")
                      a(href='#')
                        span.pull-right (50)
                        | {{ans.brname}}
                    
              // /brands_products
              .price-range
                // price-range
                h2 Price Range
